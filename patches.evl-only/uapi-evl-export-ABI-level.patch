---
 include/evl/control.h      | 2 --
 include/uapi/evl/control.h | 2 ++
 2 files changed, 2 insertions(+), 2 deletions(-)

diff --git a/include/evl/control.h b/include/evl/control.h
index 14f22fb1778c..5a736ab2e5ea 100644
--- a/include/evl/control.h
+++ b/include/evl/control.h
@@ -11,8 +11,6 @@
 #include <linux/notifier.h>
 #include <evl/factory.h>
 
-#define EVL_ABI_LEVEL  0
-
 enum evl_run_states {
 	EVL_STATE_DISABLED,
 	EVL_STATE_RUNNING,
diff --git a/include/uapi/evl/control.h b/include/uapi/evl/control.h
index 2ed03b07d0d5..55191cfc843a 100644
--- a/include/uapi/evl/control.h
+++ b/include/uapi/evl/control.h
@@ -10,6 +10,8 @@
 #include <linux/types.h>
 #include <uapi/evl/sched.h>
 
+#define EVL_ABI_LEVEL  0
+
 struct evl_core_info {
 	__u32 abi_level;
 	__u32 fpu_features;
